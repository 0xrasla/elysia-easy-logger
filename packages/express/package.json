{
  "name": "@rasla/express-logify",
  "version": "1.0.0",
  "description": "A beautiful, fast, and type-safe logging middleware for Express.js applications",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "LICENSE.md",
    "README.md"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "format": "prettier --write \"src/**/*.ts\" \"examples/**/*.ts\"",
    "lint": "eslint \"src/**/*.ts\" --fix",
    "test": "jest",
    "prepare": "npm run build"
  },
  "keywords": [
    "express",
    "logger",
    "middleware",
    "logging",
    "typescript",
    "console-log",
    "file-log",
    "http-logger"
  ],
  "author": {
    "name": "0xRasla",
    "email": "programmer.rasla@gmail.com",
    "url": "https://github.com/0xRasla"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/0xRasla/logify.git"
  },
  "bugs": {
    "url": "https://github.com/0xRasla/logify/issues"
  },
  "homepage": "https://github.com/0xRasla/logify#readme",
  "license": "MIT",
  "dependencies": {
    "chalk": "^4.1.2",
    "express": "^4.18.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.5",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "eslint": "^8.56.0",
    "prettier": "^3.1.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.3"
  },
  "peerDependencies": {
    "express": ">=4.17.1"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
